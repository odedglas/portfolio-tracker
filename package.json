{
  "name": "portfolio-tracker",
  "version": "1.0.0",
  "description": "Track it!",
  "productName": "Portfolio Tracker.",
  "author": "Oded Goldglas <oded.godglas@fiverr.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\"",
    "test": "node ./test.js",
    "dev": "quasar dev -m pwa",
    "firebase:build": "cd functions && npm run build",
    "firebase:dev": "cd functions && npm run serve",
    "build": "quasar build -m pwa",
    "build:sw": "tsx ./service-worker.build.ts",
    "postbuild": "cp -r src/assets/* dist/pwa/assets && npm run build:sw",
    "predeploy": "npm run build",
    "predeploy-h": "npm run build",
    "predeploy-f": "npm run firebase:build",
    "deploy": "firebase deploy",
    "deploy-h": "firebase deploy --only hosting",
    "deploy-f": "firebase deploy --only functions",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "pre-commit": [
    "precommit-msg",
    "format"
  ],
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "apexcharts": "^3.49.1",
    "axios": "^1.2.1",
    "firebase": "^10.11.1",
    "pinia": "^2.0.11",
    "quasar": "^2.8.0",
    "swiper": "^11.1.4",
    "vue": "^3.5.10",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.12",
    "vue3-apexcharts": "^1.5.3"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.8.0",
    "@types/lodash": "^4.17.4",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "dotenv": "^16.4.5",
    "esbuild-wasm": "latest",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "firebase-tools": "^13.7.5",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^4.5.4",
    "vite-plugin-checker": "^0.6.4",
    "vue-tsc": "^1.8.22",
    "workbox-build": "^7.1.1",
    "workbox-cacheable-response": "^7.1.0",
    "workbox-core": "^7.1.0",
    "workbox-expiration": "^7.1.0",
    "workbox-precaching": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0"
  },
  "overrides": {
    "esbuild": "npm:esbuild-wasm@latest"
  },
  "engines": {
    "node": "^20 || ^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
